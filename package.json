{
  "name": "auth-system",
  "version": "1.0.0",
  "description": "Full-stack authentication system with Next.js frontend and Express backend",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd Frontend/SRC && npm run dev",
    "dev:backend": "cd Backend/SRC && npm run dev",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd Frontend/SRC && npm run start",
    "start:backend": "cd Backend/SRC && npm run start",
    "build": "cd Frontend/SRC && npm run build",
    "install:all": "npm install && cd Frontend/SRC && npm install && cd ../Backend/SRC && npm install",
    "install:frontend": "cd Frontend/SRC && npm install",
    "install:backend": "cd Backend/SRC && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "keywords": [
    "authentication",
    "nextjs",
    "express",
    "fullstack"
  ],
  "author": "",
  "license": "ISC"
}